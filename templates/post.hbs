{{#*inline "page"}}
<section id="{{post.title}}" class="white">
  <div class="w-100 mw-none ph3 mw8-m mw9-l center f3">
    <header>
      <h2>{{ post.title }}</h2>
      <div class="highlight"></div>
      <p class="publish_date">posted: {{post.year}}-{{post.month}}-{{post.day}}</p>
    </header>

    <div class="post">
      {{{ post.contents }}}
    </div>
  </div>
</section>
{{/inline}}
{{~> (parent)~}}
